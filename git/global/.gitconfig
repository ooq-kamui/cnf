[user]
  name  = ooq-tiki
  email = ooq.tiki@gmail.com
[color "diff"]
  old = magenta
[color "status"]
  added     = cyan
  changed   = yellow
  untracked = yellow
  unmerged  = yellow
[core]
  pager = less -x2
  editor = vim
[pull]
  rebase = false
[init]
  defaultBranch = main
[merge]
  tool = vimdiff
[alias]
  s     = status
  a     = add
  ao    = add .
  ooa   = add ..
  oooa  = add ../..
  ooooa = add ../../..
  b     = branch
  l     = log
  ch    = checkout
  co    = commit
  d     = diff
  dw    = diff
  ds    = diff --staged
  ls    = diff --staged --name-only
  lw    = diff          --name-only
  mrg   = merge
  psh   = push
  pll   = pull
  cnf   = config
[log]
  date = iso-local

