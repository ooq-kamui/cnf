[user]
  name  = ooq-tiki
  email = ooq.tiki@gmail.com
[color "diff"]
  old = magenta
[color "status"]
  # added     = magenta
  changed   = yellow
  untracked = yellow
  unmerged  = yellow
[core]
pager = less -x2
  editor = vim
[pull]
  rebase = false
[init]
  defaultBranch = main
[merge]
  tool = vimdiff
[alias]
  s = status
  a     = add
  ao    = add .
  ooa   = add ..
  oooa  = add ../..
  ooooa = add ../../..
  b = branch
  l = log
  ch = checkout
  co = commit
  psh = push
  pll = pull
  mrg = merge
  d   = diff
  dw  = diff
  ds  = diff --staged
  ssl = diff --staged --name-only
  swl = diff          --name-only
  cnf = config
[log]
  date = iso-local

