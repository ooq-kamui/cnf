#!/bin/bash

cnf_bash_dir=~/wrk/cnf/sh/bash

cmd_dir=$cnf_bash_dir/cmd

cnf_dir=$cnf_bash_dir/cnf
cnf_file_path=$cnf_dir/.bashrc_c9

cmdsub="$1"

if   test $cmdsub = 'cd'
then
  cd $cnf_dir

elif test $cmdsub = 'vi'
then
  vi -p $cnf_file_path

elif test $cmdsub = 'src'
then
  src $cnf_file_path

elif test $cmdsub = 'slf'
then
  slf_path=$cmd_dir/cnf

  vi -p $slf_path

else
  echo "cmdsub : $cmdsub ?"
fi

